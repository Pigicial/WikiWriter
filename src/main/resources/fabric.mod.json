      "me.pigicial.wikiwriter.WikiWriter"
    ],
    "modmenu": [
      "me.pigicial.wikiwriter.config.ModMenuIntegration"
    ]
  },

  "mixins": [
    "wikiwriter.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.15.11",
    "fabric": "*",
    "minecraft": "~1.21",
    "java": ">=21",
    "yet_another_config_lib_v3": "*"
  }
}
