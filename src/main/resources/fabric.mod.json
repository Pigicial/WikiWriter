{
  "schemaVersion": 1,
  "id": "wikiwriter",
  "version": "${version}",

  "name": "WikiWriter",
  "description": "Wiki Editor tools for the official Hypixel SkyBlock Wiki. Type /wikiwriter or /ww to get started.",
  "authors": [
    "Pigicial"
  ],

  "license": "CC0-1.0",
  "icon": "assets/wikiwriter/icon.png",

  "environment": "client",
  "entrypoints": {
